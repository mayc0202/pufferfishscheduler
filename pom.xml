        <nacos.version>1.2.1</nacos.version> <!--Nacos-->
        <hystrix.version>2.2.1.RELEASE</hystrix.version> <!--Hystrix-->
        <poi-tl.version>1.12.1</poi-tl.version> <!--poi-tl-->
        <kettle.revision>9.2.0.0-290</kettle.revision> <!-- kettle -->
    </properties>

